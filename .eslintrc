{
    "env": {
        "browser": true,
        "es6": true
    },    
    "extends": [
        "eslint:recommended",
        "react-app"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "rules": {
        "semi": "error",
        "no-console": "off",
        "curly": "warn",
        "strict": ["error", "global"],
        "react/prop-types": "warn",
        "react/react-in-jsx-scope": "off"
    }
}


